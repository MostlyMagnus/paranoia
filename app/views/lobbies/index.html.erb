
<h1>I has lobbies</h1> 

<table border=1>
  <tr>
    <th>id</th>
    <th>Name</th>
    <th>Description</th>
    <th>min slots</th>
    <th>max slots</th>
    <th></th>
  </tr>
 
<% @lobbies.each do |lobby| %>
  <tr>
    <td><%= lobby.id %></td>
    <td><%= lobby.name %></td>
    <td><%= lobby.description %></td>
    <td><%= lobby.min_slots %></td>
    <td><%= lobby.max_slots %></td>
    <td><%= link_to 'join game', 'lobbies/1' %></td>
  </tr>
<% end %>
</table>
<br />




