<%= form_for @pawn do |f| %>
  
  <div><%= f.hidden_field :gamestate_id %></div>
  <div><%= f.hidden_field :role %></div>  
  <div><%= f.hidden_field :user_id %></div>
  
  <div><%= f.select :gamestate_id, [Gamestate.all.each] %></div>
  
  <div class="actions"><%= f.submit "Join" %></div>
<% end %>

